{
  "config": {
    "name": "Testing GraphQL EndPoint",
    "url": "http://localhost:4000/graphql",
    "selectedQueries": ["signin", "signup", "user"],
    "queryFile": true,
    "withMutations": false,
    "duration": 1,
    "arrivalRate": 1,
    "withOutput": true,
    "outputFolder": "tests-report",
    "headers": {
      "Authorization": "bearer <TOKEN>"
    },
    "schema": {
      "filename": "schema.graphql",
      "method": "POST",
      "json": false
    }
  },
  "args": {
    "signin": {
      "email": "test@test.com",
      "password": "123456"
    },
    "signup": {
      "email": "test@test.com",
      "password": "123456",
      "firstName": "John",
      "secondName": "Smith"
    },
    "user": {
      "id": 1
    }
  }
}
